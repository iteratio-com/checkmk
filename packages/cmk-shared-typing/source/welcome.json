{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "title": "Welcome page",
    "properties": {
        "urls": {
            "type": "object",
            "$ref": "#/$defs/welcome_urls"
        }
    },
    "required": ["urls"],
    "$defs": {
        "welcome_urls": {
            "type": "object",
            "properties": {
                "checkmk_ai": { "type": "string" },
                "checkmk_docs": { "type": "string" },
                "checkmk_forum": { "type": "string" }
            },
            "required": ["checkmk_ai", "checkmk_docs", "checkmk_forum"]
        }
    }
}
